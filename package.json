{
  "name": "nb-wms-frontend",
  "version": "0.1.0",
  "scripts": {
    "bs:build": "bsb -make-world",
    "bs:clean": "bsb -clean-world",
    "build": "bsb -make-world && parcel build index.html",
    "dev": "parcel index.html",
    "start": "bsb -make-world -w -ws _ ",
    "clean": "bsb -clean-world",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "firebase deploy --only firestore:rules,hosting",
    "local-deploy": "firebase serve --only hosting"
  },
  "keywords": [
    "BuckleScript",
    "ReasonReact",
    "reason-react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "firebase": "7.14.2",
    "firebaseui": "4.5.0",
    "gentype": "3.22.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "reason-react": "0.7.1"
  },
  "devDependencies": {
    "bs-platform": "7.2.2",
    "parcel-bundler": "1.12.4"
  }
}
